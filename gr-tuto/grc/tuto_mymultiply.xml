<?xml version="1.0"?>
<block>
  <name>mymultiply</name>
  <key>tuto_mymultiply</key>
  <category>tuto</category>
  <import>import tuto</import>
  <make>tuto.mymultiply()</make>
  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>float</type>
    <nports>2</nports>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>float</type>
  </source>
</block>
